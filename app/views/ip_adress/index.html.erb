<h1>Listing ip_adresses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ip_adresses.each do |ip_adress| %>
      <tr>
        <td><%= link_to 'Show', ip_adress %></td>
        <td><%= link_to 'Edit', edit_ip_adress_path(ip_adress) %></td>
        <td><%= link_to 'Destroy', ip_adress, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ip adress', new_ip_adress_path %>
